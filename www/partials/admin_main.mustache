{{#admin_group_admin}}

<style>
.hearts::after{content:'ðŸ¤•';animation-name: hearts;animation-duration:1s}
.emojianim::after{animation-timing-function: linear;animation-iteration-count: infinite}
@keyframes hearts{10%{content:'ðŸ˜©'}20%{content:'ðŸ˜«'}30%{content:'ðŸ˜ª'}40%{content:'ðŸ˜´'}50%{content:'ðŸ˜µ'}60%{content:'ðŸ˜¦'}70%{content:'ðŸ˜¢'}80%{content:'ðŸ˜­'}90%{content:'ðŸ¤’'}}
.a_reset{font-family:Arial!important;font-size:14px!important}
.a_h2{font-size:5em!important;font-weight:700!important}
.a_h3{font-size:4em!important;font-weight:700!important}
.a_h4{font-size:3em!important;font-weight:700!important}
.a_h5{font-size:2em!important;font-weight:700!important}
.alzaborder{border:2px solid green;border-radius:5px}
