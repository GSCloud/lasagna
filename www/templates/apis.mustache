</head>
<body>
<noscript>{{>noscript}}</noscript>
{{>gscloud_functions}}
<div class="container">
  <h1>{{ l.title }} REST API</h1>
  <ol>
{{#apis}}
  <li>
    <p>
      <b>{{ desc }}</b>
      <br>
      {{#linkit}}
      <a target="_blank" class="blue-text bold" rel="nolink" href="{{ base }}{{ path }}">{{ base }}{{ path }}</a>
      {{/linkit}}
      {{^linkit}}
      <b class="blue-text" rel="nolink" href="{{ base }}{{ path }}">{{ base }}{{ path }}</b>
      {{/linkit}}
      {{#count}}
      <br>
      <br>
      <i>Example:</i>
      {{/count}}
      {{#exam}}
      <br>
      <a target="_blank" rel="nolink" href="{{ base }}{{ . }}">{{ base }}{{ . }}</a>
      {{/exam}}
      <i>{{ info }}</i>
    </p>
  </li>
{{/apis}}
  </ol>
</div>
</body>
</html>