        var ui = $.summernote.ui;
        var list = ui.buttonGroup([
          ui.button({
            className: "dropdown-toggle",
            contents: '<span title="{{ l.a_summer_history_ttip }}" class="fas fa-history"></span> <span class="caret"></span>',
