                  s += '<span title="ğŸ—‘ {{ l.a_delete_tit }}" style="font-size:1.5rem" class="mobilebutt left hover delete" data-fig="' + t + '" data-id="' + name1 + '">âŒ</span>&nbsp;';
                  s += '<span title="ğŸ“‹ {{ l.a_copy_tit }} ' + name1 + '" style="font-size:1.2rem" class="mobilebutt extrapadding right hover copydata" data-text="{{ BASE }}upload/' + name1 + '">' + ext + 'ğŸ“‹</span>';
