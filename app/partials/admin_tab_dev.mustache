<div id="dev" class="row">
  <div class="container">

    <h4><i class="fa-solid fa-user-secret"></i>&nbsp;&nbsp;{{ l.a_dev }}</h4>
    <p>{{ l.a_dev_info }}</p>

    <a
      id="cssdebugger"
      style="margin:2px!important;"
      class="btn waves-light tooltipped white-text {{#COOKIE.debug}}red{{/COOKIE.debug}}{{^COOKIE.debug}}grey{{/COOKIE.debug}}"
      data-position="top"
      data-tooltip="{{ l.a_debugger_ttip }}"
      ><i class="fa-brands fa-css"></i>&nbsp;<i class="fa-solid fa-bug"></i>&nbsp;{{ l.a_cssdebugger }}</a>

    {{#dbg}}
    {{#admin_group_admin}}
    <a
      id="phpdebugger"
      style="margin:2px!important;"
      class="btn waves-light tooltipped white-text {{#COOKIE.tracy-debug}}red{{/COOKIE.tracy-debug}}{{^COOKIE.tracy-debug}}grey{{/COOKIE.tracy-debug}}"
      data-position="top"
      data-tooltip="{{ l.a_debugger_ttip }}"
      ><i class="fa-brands fa-php"></i>&nbsp;<i class="fa-solid fa-bug"></i>&nbsp;{{ l.a_phpdebugger }}</a>
    {{/admin_group_admin}}
    {{/dbg}}

  </div>
</div>
