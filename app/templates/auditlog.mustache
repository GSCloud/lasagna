<!doctype html>
<html class="no-js" lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="x-ua-compatible" content="IE=edge">
<title>Audit LOG</title>
<script>
'use strict';
// tweaks
(function() {
  var externallyFramed = false;
  try {
    externallyFramed = top.location.host != location.host;
  }
  catch(err) {
    externallyFramed = true;
  }
  if(externallyFramed) {
    top.location = location;
  }
})();
!(function(a){function e(){}for(var b="assert clear count debug dir dirxml error exception group groupCollapsed groupEnd info log markTimeline profile profileEnd table time timeEnd timeline timelineEnd timeStamp trace warn".split(" "),c=b.length,d=a.console=a.console||{};c--;)a=b[c],d[a]||(d[a]=e)})(window);
</script>
{{#host}}{{#base}}<base href="{{ base }}">{{/base}}{{/host}}
{{#canonical_url}}<link rel="canonical" href="{{ canonical_url }}{{ request_path }}">{{/canonical_url}}
{{>head_meta}}
{{>head_favicons}}
{{>head_umami}}
{{>head_html5shim}}
<style>
/* Material icons */
@font-face {
  font-family: 'Material Icons';
  font-style: normal;
  font-weight: 400;
  src: local('Material Icons'),
       local('materialicons-regular'),
       url(https://cdn.gscloud.cz/webfonts/materialicons-regular.woff2) format('woff2'),
       url(https://cdn.gscloud.cz/webfonts/materialicons-regular.woff) format('woff'),
       url(https://cdn.gscloud.cz/webfonts/materialicons-regular.ttf) format('truetype');
}
.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px;
  display: inline-block;
  line-height: 1;
  text-transform: none;
  letter-spacing: normal;
  word-wrap: normal;
  white-space: nowrap;
  direction: ltr;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  font-feature-settings: 'liga';
}

/* ADMIN LOG */
.alogcol2 {font-weight:bold}
td,th {padding:2px!important}
</style>
<link rel="stylesheet" href="https://cdn.gscloud.cz/css/materialize.min.css">
<script src="https://cdn.gscloud.cz/js/jquery.min.js"></script>
</head>
<body>
<table class="highlight">
<thead style="border-bottom:4px solid black!important">
<tr>
<th>Date</th>
<th>Operation</th>
<th>Address</th>
<th>Name</th>
<th>E-mail</th>
</tr>
</thead>
<tbody>
{{#content}}
<tr>{{.}}</tr>
{{/content}}
</tbody>
</table>

<script>

// modify timestamp
$(".alogcol1").each(function() {
  var x = $(this).html();
  x= x.replace('T', '. ').replace('-', '<br>').replace('-', '.').replace('+', ' +');
  $(this).html(x);
});

// modify timestamp
var width = document.documentElement.clientWidth;

if (width < 800) $(".alogcol2").each(function() {
  var x = $(this).html();
  if (width < 800) x= x.substring(x, 50);
  if (width < 400) x= x.substring(x, 30);
  $(this).html(x);
});

if (width < 800) $(".alogcol3").each(function() {
  var x = $(this).html();
  if (width < 800) x= x.substring(x, 50);
  if (width < 400) x= x.substring(x, 30);
  $(this).html(x);
});

</script>
</body>
</html>